{
  "name": "knip",
  "version": "0.7.1",
  "description": "Find unused files, dependencies and exports in your TypeScript and JavaScript project",
  "keywords": [
    "find",
    "detect",
    "unused",
    "unreferenced",
    "files",
    "dependencies",
    "exports",
    "types",
    "duplicates",
    "typescript",
    "maintenance",
    "unimported"
  ],
  "repository": "github:webpro/knip",
  "homepage": "https://github.com/webpro/knip",
  "bugs": "https://github.com/webpro/knip/issues",
  "main": "dist/index.js",
  "bin": {
    "knip": "dist/cli.js"
  },
  "scripts": {
    "knip": "node ./dist/cli.js",
    "test": "node --loader tsx --test test/*.spec.ts",
    "watch": "tsc --watch",
    "build": "rm -rf dist && tsc",
    "prepublishOnly": "npm test && npm run build && npm run knip",
    "release": "release-it"
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "Lars Kappert",
    "email": "lars@webpro.nl"
  },
  "license": "ISC",
  "dependencies": {
    "@snyk/github-codeowners": "1.0.0",
    "globby": "13.1.2",
    "is-builtin-module": "3.2.0",
    "micromatch": "4.0.5",
    "ts-morph": "16.0.0",
    "ts-morph-helpers": "0.5.1"
  },
  "devDependencies": {
    "@types/micromatch": "4.0.2",
    "@types/node": "18.8.5",
    "prettier": "2.7.1",
    "release-it": "15.5.0",
    "tsx": "3.10.1",
    "typescript": "4.8.4"
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "engines": {
    "node": ">=16.17.0"
  },
  "engineStrict": true
}
