{
  "name": "knip",
  "version": "1.0.0-beta.4",
  "description": "Find unused files, dependencies and exports in your TypeScript and JavaScript project",
  "keywords": [
    "find",
    "detect",
    "unused",
    "unreferenced",
    "missing",
    "files",
    "dependencies",
    "exports",
    "types",
    "duplicates",
    "typescript",
    "maintenance",
    "unimported",
    "class",
    "enum",
    "members",
    "workspace",
    "workspaces",
    "monorepo"
  ],
  "repository": "github:webpro/knip",
  "homepage": "https://github.com/webpro/knip",
  "bugs": "https://github.com/webpro/knip/issues",
  "main": "dist/index.js",
  "bin": {
    "knip": "dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "preknip": "npm run build",
    "knip": "node ./dist/cli.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "globstar -- node --loader tsx --test \"test/**/*.spec.ts\"",
    "watch": "tsc --watch",
    "build": "rm -rf dist && tsc",
    "prepublishOnly": "npm test && npm run build && npm run knip",
    "format": "remark README.md -o",
    "release": "release-it",
    "postinstall": "patch-package",
    "add-plugin:help": "echo 'Usage: npm run add-plugin --name=[name] (and update README.md and schema.json)'",
    "add-plugin": "cp -R src/plugins/_template src/plugins/$npm_config_name && echo \"export * as $npm_config_name from \\\"./$npm_config_name/index.js\\\";\" >> src/plugins/index.ts"
  },
  "files": [
    "dist",
    "schema.json"
  ],
  "author": {
    "name": "Lars Kappert",
    "email": "lars@webpro.nl"
  },
  "license": "ISC",
  "dependencies": {
    "@esbuild-kit/esm-loader": "2.5.4",
    "@npmcli/map-workspaces": "3.0.1",
    "@snyk/github-codeowners": "1.1.0",
    "chalk": "5.2.0",
    "easy-table": "1.2.0",
    "esbuild": "0.16.12",
    "esbuild-register": "3.4.2",
    "eslint": "8.30.0",
    "fast-glob": "3.2.12",
    "globby": "13.1.3",
    "js-yaml": "4.1.0",
    "micromatch": "4.0.5",
    "nano-memoize": "2.0.0",
    "patch-package": "6.5.0",
    "pretty-ms": "8.0.0",
    "strip-json-comments": "5.0.0",
    "summary": "2.1.0",
    "ts-morph": "17.0.1",
    "ts-morph-helpers": "0.6.3",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@jest/types": "29.3.1",
    "@npmcli/package-json": "3.0.0",
    "@types/eslint": "8.4.10",
    "@types/js-yaml": "4.0.5",
    "@types/micromatch": "4.0.2",
    "@types/node": "18.11.18",
    "@types/npmcli__map-workspaces": "3.0.0",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "globstar": "1.0.0",
    "release-it": "15.6.0",
    "remark-cli": "11.0.0",
    "remark-preset-webpro": "0.0.1",
    "tsx": "3.12.1",
    "type-fest": "3.5.0",
    "typescript": "4.9.4"
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-webpro"
    ]
  },
  "engines": {
    "node": ">=16.17.0 <17 || >=18.6.0"
  },
  "engineStrict": true
}
