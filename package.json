{
  "name": "knip",
  "version": "1.0.0-alpha.2",
  "description": "Find unused files, dependencies and exports in your TypeScript and JavaScript project",
  "keywords": [
    "find",
    "detect",
    "unused",
    "unreferenced",
    "files",
    "dependencies",
    "exports",
    "types",
    "duplicates",
    "typescript",
    "maintenance",
    "unimported"
  ],
  "repository": "github:webpro/knip",
  "homepage": "https://github.com/webpro/knip",
  "bugs": "https://github.com/webpro/knip/issues",
  "main": "dist/index.js",
  "bin": {
    "knip": "dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "knip": "node ./dist/cli.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "globstar -- node --loader tsx --test \"test/*.spec.ts\"",
    "watch": "tsc --watch",
    "build": "rm -rf dist && tsc",
    "prepublishOnly": "npm test && npm run build && npm run knip",
    "format": "remark README.md -o",
    "release": "release-it"
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "Lars Kappert",
    "email": "lars@webpro.nl"
  },
  "license": "ISC",
  "dependencies": {
    "@esbuild-kit/esm-loader": "2.5.1",
    "@snyk/github-codeowners": "1.0.0",
    "easy-table": "1.2.0",
    "esbuild": "0.15.15",
    "esbuild-register": "3.4.1",
    "eslint": "8.28.0",
    "globby": "13.1.2",
    "js-yaml": "4.1.0",
    "micromatch": "4.0.5",
    "strip-json-comments": "5.0.0",
    "summary": "2.1.0",
    "ts-morph": "17.0.1",
    "ts-morph-helpers": "0.6.3",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@jest/types": "29.3.1",
    "@types/eslint": "8.4.10",
    "@types/js-yaml": "4.0.5",
    "@types/micromatch": "4.0.2",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "globstar": "1.0.0",
    "release-it": "15.5.0",
    "remark-cli": "11.0.0",
    "remark-preset-webpro": "0.0.1",
    "tsx": "3.12.1",
    "type-fest": "3.2.0",
    "typescript": "4.9.3"
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-webpro"
    ]
  },
  "engines": {
    "node": ">=16.17.0 <17 || >=18.6.0"
  },
  "engineStrict": true
}
