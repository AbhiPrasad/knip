{
  "$schema": "https://unpkg.com/knip@2/schema.json",
  "ignoreWorkspaces": ["."],
  "workspaces": {
    "packages/knip": {
      "entry": ["src/{index,cli}.ts!"],
      "project": ["src/**/*.ts!"],
      "ignore": ["**/fixtures", "**/_template"],
      "ignoreDependencies": ["@pnpm/logger"],
      "node-test-runner": {
        "entry": ["test/**/*.test.ts"],
        "project": ["test/**/*.ts"]
      }
    }
  }
}
