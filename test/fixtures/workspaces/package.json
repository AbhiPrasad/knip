{
  "name": "workspaces",
  "workspaces": [
    "docs",
    "apps/*",
    "packages/*"
  ],
  "knip": {
    "workspaces": {
      ".": {
        "entry": [],
        "project": []
      },
      "*": {
        "entry": "index.ts",
        "project": "**/*.ts"
      },
      "apps/*": {
        "entry": "index.ts",
        "project": "**/*.ts"
      },
      "packages/*": {
        "entry": "index.ts",
        "project": [
          "**/*.ts",
          "!ignored/*.ts"
        ]
      }
    }
  },
  "dependencies": {
    "cypress": "*",
    "root-dependency": "*",
    "unused-dependency": "*"
  }
}
